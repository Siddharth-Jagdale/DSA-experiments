#include <stdio.h>
#include <string.h>

// Binary Search function
int binarySearch(char playlist[][50], int n, char song[]) {
    int low = 0, high = n - 1;

    while (low <= high) {
        int mid = (low + high) / 2;
        int cmp = strcmp(song, playlist[mid]);

        if (cmp == 0) {
            return mid; // Found
        } else if (cmp < 0) {
            high = mid - 1; // Search left
        } else {
            low = mid + 1; // Search right
        }
    }
    return -1; // Not found
}

int main() {
    int n;
    char song[50];

    printf("Enter number of songs: ");
    scanf("%d", &n);

    char playlist[n][50];
    printf("Enter songs in alphabetical order:\n");
    for (int i = 0; i < n; i++) {
        scanf("%s", playlist[i]);
    }

    printf("Enter song to search: ");
    scanf("%s", song);

    int result = binarySearch(playlist, n, song);

    if (result != -1)
        printf("Song '%s' found at position %d.\n", song, result + 1);
    else
        printf("Song '%s' not found in playlist.\n", song);

    return 0;
}

Output:
Enter number of songs: 5
Enter songs in alphabetical order:
dhun
golden
trolly
saphire
kpop
Enter song to search: believer
Song 'believer' not found in playlist.
